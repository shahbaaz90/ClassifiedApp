<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="ClassifiedApp2.Views.RootTabbedPage"
    xmlns:local="clr-namespace:ClassifiedApp2.Views"
    Title="{Binding Title}">
    <TabbedPage.Children>
        <local:Tab1HomePage Icon="HomeIcon.png" />
        <local:Tab2AlertsPage Icon="AlertsIcon.png" />
        <local:Tab3ChatPage Icon="ChatsIcon.png" />
        <local:Tab4ProfilePage Icon="ProfileIcon.png" />
    </TabbedPage.Children>
</TabbedPage>