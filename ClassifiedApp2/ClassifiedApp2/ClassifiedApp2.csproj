<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Prism.DryIoc.Forms" Version="7.0.0.396" />
    <PackageReference Include="Xamarin.Forms" Version="2.5.0.280555" />
    <PackageReference Include="akavache.sqlite3" Version="6.0.0-alpha0038" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Services\" />
    <Folder Include="Models\" />
    <Folder Include="Views\" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\ClassifiedApp2.Common\ClassifiedApp2.Common.csproj" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Views\RootTabbedPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Include="Views\Tab1HomePage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Include="Views\Tab2AlertsPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Include="Views\Tab3ChatPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Include="Views\Tab4ProfilePage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="Views\RootTabbedPage.xaml.cs">
      <DependentUpon>RootTabbedPage.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\Tab2AlertsPage.xaml.cs">
      <DependentUpon>Tab2AlertsPage.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\Tab3ChatPage.xaml.cs">
      <DependentUpon>Tab3ChatPage.xaml</DependentUpon>
    </Compile>
    <Compile Update="Views\Tab4ProfilePage.xaml.cs">
      <DependentUpon>Tab4ProfilePage.xaml</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>